{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": "Equity's Projects"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "constructionStart": {
      "type": "date"
    },
    "constructionEnd": {
      "type": "date"
    },
    "floors": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "landSize": {
      "type": "string"
    },
    "landFacing": {
      "type": "enumeration",
      "enum": [
        "East",
        "West",
        "South",
        "North",
        "Corner Plot",
        "East-West",
        "North-South"
      ]
    },
    "roadWidth": {
      "type": "decimal"
    },
    "youtubeVideos": {
      "type": "component",
      "repeatable": true,
      "component": "project.youtube-video"
    },
    "projectDocuments": {
      "type": "component",
      "repeatable": true,
      "required": false,
      "component": "project.document-item"
    },
    "photos": {
      "type": "component",
      "repeatable": true,
      "required": false,
      "component": "project.photo-item"
    },
    "bulkPhotoUploads": {
      "type": "component",
      "repeatable": true,
      "required": false,
      "component": "project.bulk-photo-upload"
    },
    "brochure": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "Unit": {
      "type": "component",
      "repeatable": true,
      "component": "project.unit-types"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "project_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-type.project-type",
      "inversedBy": "projects"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "projects"
    },
    "project_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project-status.project-status"
    },
    "parking_floors": {
      "type": "integer"
    },
    "parking_spaces": {
      "type": "integer"
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "project.feature-item"
    },
    "amenities": {
      "type": "component",
      "repeatable": true,
      "component": "project.amenity-item"
    },
    "constructionUpdates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::construction-update.construction-update",
      "mappedBy": "project"
    },
    "documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::document.document",
      "mappedBy": "project"
    }
  }
}
