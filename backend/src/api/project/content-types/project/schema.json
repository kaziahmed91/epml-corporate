{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": "Equity's Projects"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "constructionStart": {
      "type": "date"
    },
    "constructionEnd": {
      "type": "date"
    },
    "floors": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "landSize": {
      "type": "string"
    },
    "landFacing": {
      "type": "enumeration",
      "enum": [
        "East",
        "West",
        "South",
        "North",
        "Corner Plot",
        "East-West",
        "North-South"
      ]
    },
    "roadWidth": {
      "type": "decimal"
    },
    "youtubeVideos": {
      "type": "component",
      "component": "project.youtube-video",
      "repeatable": true
    },
    "photos": {
      "type": "component",
      "component": "project.photo-item",
      "repeatable": true,
      "required": false
    },
    "bulkPhotoUploads": {
      "type": "component",
      "component": "project.bulk-photo-upload",
      "repeatable": true,
      "required": false
    },
    "Unit": {
      "type": "component",
      "component": "project.unit-types",
      "repeatable": true
    },
    "commercialFloors": {
      "type": "component",
      "component": "project.project-floor",
      "repeatable": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "project_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-type.project-type",
      "inversedBy": "projects"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "projects"
    },
    "project_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project-status.project-status"
    },
    "parking_floors": {
      "type": "integer"
    },
    "parking_spaces": {
      "type": "integer"
    },
    "features": {
      "type": "component",
      "component": "project.feature-item",
      "repeatable": true
    },
    "amenities": {
      "type": "component",
      "component": "project.amenity-item",
      "repeatable": true
    },
    "constructionUpdates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::construction-update.construction-update",
      "mappedBy": "project"
    },
    "documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::document.document",
      "mappedBy": "project"
    }
  }
}
