{
  "kind": "collectionType",
  "collectionName": "leads",
  "info": {
    "singularName": "lead",
    "pluralName": "leads",
    "displayName": "Lead",
    "description": "Customer inquiries and leads"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "source": {
      "type": "enumeration",
      "enum": ["website", "facebook", "google", "referral", "walk_in", "phone"]
    },
    "inquiryType": {
      "type": "enumeration",
      "enum": [
        "general",
        "project_specific",
        "landowner",
        "career",
        "media",
        "complaint"
      ]
    },
    "message": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": ["new", "contacted", "qualified", "converted", "closed", "lost"],
      "default": "new"
    },
    "assignedTo": {
      "type": "string"
    },
    "followUpDate": {
      "type": "datetime"
    },
    "notes": {
      "type": "json"
    },
    "leadReference": {
      "type": "string",
      "unique": true
    },
    "utmSource": {
      "type": "string"
    },
    "utmMedium": {
      "type": "string"
    },
    "utmCampaign": {
      "type": "string"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "leads"
    },
    "unitInterest": {
      "type": "string"
    }
  }
}
