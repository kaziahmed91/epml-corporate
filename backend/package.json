{
  "name": "epml-strapi",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "seed:bootstrap": "node scripts/seed-data.js",
    "seed:all": "SEED_ENABLE=true yarn develop",
    "seed:force": "SEED_ENABLE=true FORCE_RESEED=true yarn develop",
    "seed:prod": "NODE_ENV=production FORCE_PRODUCTION_SEED=true yarn start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.14.0",
    "@strapi/plugin-users-permissions": "5.14.0",
    "@strapi/strapi": "5.14.0",
    "commander": "^11.0.0",
    "dotenv": "^16.5.0",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0",
    "tsx": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
